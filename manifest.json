{
  "manifest_version": 2,

  "name": "Jisho2Sheets",
  "description": "Grabs data from Jisho and imports it into a Google Sheet.",
  "version": "1.0",
  "options_page" : "settings_page.html",

  "browser_action": {
    "default_icon": "img/j2g_logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*.jisho.org/",
    "https://script.google.com/"
  ]
}